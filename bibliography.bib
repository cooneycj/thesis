	Keywords = {constraint theory, convergence of numerical methods, matrix decomposition, optimisationbound-constrained optimization problems, convergence, multiplicative update algorithms, nonnegative matrix factorization},
	Number = {6},
	Pages = {1589-1596},
	Title = {On the Convergence of Multiplicative Update Algorithms for Nonnegative Matrix Factorization},
	Volume = {18},
