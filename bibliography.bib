	Journal = {Science},
	Number = {5297},
	Pages = {209-213},
	Title = {A synaptically controlled, associative signal for Hebbian plasticity in hippocampal neurons},
	Url = {http://www.sciencemag.org/content/275/5297/209.abstract},
