@article{BialekEtAl1991a,
	Abstract = {Traditional approaches to neural coding characterize the
encoding of known stimuli in average neural responses. Organisms face
nearly the opposite task--extracting information about an unknown
time-dependent stimulus from short segments of a spike train. Here the
neural code was characterized from the point of view of the organism,
culminating in algorithms for real-time stimulus estimation based on a
single example of the spike train. These methods were applied to an
identified movement-sensitive neuron in the fly visual system. Such
decoding experiments determined the effective noise level and fault
tolerance of neural computation, and the structure of the decoding
algorithms suggested a simple model for real-time analog signal
processing with spiking neurons.},
	Author = {W Bialek and F Rieke and RR de Ruyter van Steveninck and D Warland},
	Date-Added = {2014-12-18 15:39:33 +0000},
	Date-Modified = {2014-12-18 15:39:33 +0000},
